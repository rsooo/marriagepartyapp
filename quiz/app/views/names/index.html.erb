<h1>Listing names</h1>

<table>
  <thead>
    <tr>
      <th>Q1</th>
      <th>Q2</th>
      <th>Q3</th>
      <th>Q4</th>
      <th>Q5</th>
      <th>Q6</th>
      <th>Q7</th>
      <th>Q8</th>
      <th>Q9</th>
      <th>Q10</th>
      <th>Comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @names.each do |name| %>
      <tr>
        <td><%= name.q1 %></td>
        <td><%= name.q2 %></td>
        <td><%= name.q3 %></td>
        <td><%= name.q4 %></td>
        <td><%= name.q5 %></td>
        <td><%= name.q6 %></td>
        <td><%= name.q7 %></td>
        <td><%= name.q8 %></td>
        <td><%= name.q9 %></td>
        <td><%= name.q10 %></td>
        <td><%= name.comment %></td>
        <td><%= link_to 'Show', name %></td>
        <td><%= link_to 'Edit', edit_name_path(name) %></td>
        <td><%= link_to 'Destroy', name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Name', new_name_path %>
